{
  "name": "reactjs-films-homework",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.com/SozonikOleg/reactjs-films-homework.svg?branch=master)](https://travis-ci.com/SozonikOleg/reactjs-films-homework)",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack.dev.js && SET NODE_ENV=development & node server.js",
    "build": "webpack --config webpack.prod.js",
    "prod": "npm run build && SET NODE_ENV=production & node server.js",
    "server": "node server.js",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test eslint --debug src/",
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "transformIgnorePatterns": [
        "node_modules/(?!(react-native|my-project|react-native-button)/)"
      ],
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$/": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/*jsx"
    ],
    "coverageThreshold": {
      "./src": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "exports-loader": "^0.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^7.0.2",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "antd": "^3.16.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SozonikOleg/reactjs-films-homework.git"
  },
  "author": "Sozonik Oleg",
